    "source":
        "pragma solidity ^0.4.24;\n\nimport \"openzeppelin-solidity/contracts/ownership/Ownable.sol\";\n\n\ncontract LinkedToOracle is Ownable {\n\n  /* Address of the proxyOracleRegistry */\n  address private proxyOracleAddress;\n\n\n  /* Throws if the sender is not the proxyOracleAddress */\n  modifier onlyFromOracle() {\n    require(msg.sender == proxyOracleAddress, \"Authorization denied\");\n    _;\n  }\n\n  /**\n   * @dev Registers the address of the proxy oracle registry\n   * @param _proxyOracleAddress Address of the proxy oracle registry\n   */\n  constructor(\n    address _proxyOracleAddress\n  )\n    internal\n  {\n    // Set the proxyOracleAddress\n    proxyOracleAddress = _proxyOracleAddress;\n  }\n}\n",
