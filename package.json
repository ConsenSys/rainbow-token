{
  "name": "rainbow-token",
  "homepage": "./",
  "version": "0.1.0-dev",
  "private": true,
  "description": "Rainbow Token decentralized Application",
  "repository": {
    "type": "git",
    "url": "https://github.com/ConsenSys/rainbow-token.git"
  },
  "author": "ConsenSys France",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ConsenSys/rainbow-token"
  },
  "homepage": "https://github.com/ConsenSys/rainbow-token",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "mnemonic-generator": "^1.0.0",
    "morgan": "^1.9.0",
    "react": "^16.8.3",
    "react-blockies": "^1.4.1",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-scripts": "^2.0.0",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "web3": "1.0.0-beta.33"
  },
  "devDependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.23.0",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "chai-bignumber": "^2.0.0",
    "coveralls": "^2.13.1",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "ethereumjs-util": "^5.1.2",
    "ethjs-abi": "^0.2.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "solidity-coverage": "^0.5.11",
    "solium": "^1.1.7",
    "truffle": "^5.0.6",
    "truffle-hdwallet-provider": "^1.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/src/index.js",
      "!**/src/registerServiceWorker.js"
    ]
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 4
  },
  "scripts": {
    "lint:js": "eslint .src/ ./migrations ./test",
    "lint:js:fix": "eslint ./test ./migrations --fix",
    "lint:sol": "solium -d .",
    "lint:sol:fix": "solium -d . --fix",
    "lint:all": "npm run lint:js && npm run lint:sol",
    "lint:all:fix": "npm run lint:js:fix && npm run lint:sol:fix",
    "console": "truffle console",
    "contract:test": "truffle test --network development",
    "contract:test:coverage": "solidity-coverage",
    "contract:compile": "truffle compile && cp -r contractBuild/. src/web3Config/abis/",
    "contract:migrate:dev": "truffle migrate --reset --network development && cp -r contractBuild/. src/web3Config/abis/",
    "contract:migrate:ropsten": "truffle migrate --reset --network ropsten && cp -r contractBuild/. src/web3Config/abis/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "ganache:start": "cd ganache && ACCOUNTS=`cat accounts.txt` docker-compose up",
    "ganache:stop": "cd ganache && docker-compose stop"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
